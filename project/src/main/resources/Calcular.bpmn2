          <drools:metaValue><![CDATA[Calcular promedio
]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_6AB98F16-3DCA-49DB-8709-6C15DC08DDB3</bpmn2:incoming>
      <bpmn2:outgoing>_76A6F2C4-F9CC-4DF0-8572-5F01E8F378CE</bpmn2:outgoing>
      <bpmn2:script><![CDATA[float a = (float) kcontext.getVariable("nota1");
float b = (float) kcontext.getVariable("nota2");
float c = (float) kcontext.getVariable("nota3");
float d = (float) kcontext.getVariable("nota4");
float e = (float) kcontext.getVariable("nota5");

float prom = (a + b + c + d + e) / 5;


System.out.println("El promedio es: " + prom);


if(prom <= 2.99)
{
 System.out.println(" Su calificacion fue reprobado");
}


if(prom > 3.0 && prom <= 3.99)
{
 System.out.println(" Su calificacion fue suficiente");
}


if(prom > 4.0 && prom <= 4.89)
{
 System.out.println(" Su calificacion fue bien");
}



if(prom >= 4.9)
{
 System.out.println(" Su calificacion fue excelente");
}]]></bpmn2:script>
